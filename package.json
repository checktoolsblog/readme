{
  "name": "github-profile-readme-generator",
  "private": true,
  "description": "A simple react app to generate beautiful github profile readme in md(markdown)",
  "version": "1.2.0",
  "author": "Rahul Jain <rahuldkjain@gmail.com>",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@primer/octicons-react": "^17.10.1",
    "axios": "^1.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.7",
    "enzyme-to-json": "^3.6.2",
    "gatsby": "^5.4.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-analytics": "^5.4.0",
    "gatsby-plugin-manifest": "^5.4.0",
    "gatsby-plugin-offline": "^6.4.0",
    "gatsby-plugin-react-helmet": "^6.4.0",
    "gatsby-plugin-sharp": "5.4.0",
    "gatsby-remark-prismjs": "^7.4.0",
    "gatsby-source-filesystem": "^5.4.0",
    "gatsby-transformer-remark": "^6.4.0",
    "gatsby-transformer-sharp": "^5.4.0",
    "gsap": "^3.11.4",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "babel-jest": "29.3.1",
    "babel-preset-gatsby": "3.4.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-postcss": "^6.4.0",
    "gatsby-plugin-purgecss": "^6.2.1",
    "gatsby-plugin-twitter": "^5.4.0",
    "gatsby-remark-embedder": "^6.0.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.2",
    "tailwindcss": "^3.2.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest -i -u --coverage",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahuldkjain/github-profile-readme-generator"
  },
  "bugs": {
    "url": "https://github.com/rahuldkjain/github-profile-readme-generator/issues"
  }
}
